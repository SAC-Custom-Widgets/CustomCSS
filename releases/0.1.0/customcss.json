	"id": "nkappler.customcss",
	"version": "0.1.0",
	"name": "Custom CSS, Custom Widget",
	"newInstancePrefix": "CustomCSS",
	"description": "Apply custom CSS to your application",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "sdk-customcss",
			"url": "https://widgets.nkappler.de/CustomCSS/releases/0.1.0/webcomponent.js",
			"integrity": "sha256-JfNhNgbP5MuR4kLheIVAWhXyNBZd7Y8/06zXet2GXsw=",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "sdk-customcss-aps",
			"url": "https://widgets.nkappler.de/CustomCSS/releases/0.1.0/aps_webcomponent.js",
			"integrity": "sha256-EW9ndX5Aru//suBpodvHeBBPr/vDQBrFi1cVMnBGa1c=",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"css": {
			"type": "string",
			"description": "CSS code for styling",
			"default": ".sapLumiraStoryLayoutCommonEditPanel, .sapLumiraStoryLayoutCommonPagePanel { background-color: lime !important}"
		}
	},
	"methods": {
		"getCss": {
			"returnType": "string",
			"description": "CSS code",
			"parameters": [],
			"body": "return this.css;"
		},
		"setCss": {
			"description": "CSS code",
			"parameters": [
				{
					"name": "val",
					"type": "string",
					"description": "New CSS"
				}
			],
			"body": "this.css = val;"
		}
	},
	"events": {
		"onClick": {
			"description": "CustomCss was clicked"
		}
	},
	"icon": "data:image/png;base64,"
